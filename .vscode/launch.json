{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: run app.py",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "app.py", "--server.port=8501", "--server.address=localhost"],
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": true
    }
  ]
}
